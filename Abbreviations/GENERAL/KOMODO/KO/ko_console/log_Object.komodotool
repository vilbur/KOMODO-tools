{
  "keyboard_shortcut": "", 
  "name": "koLogO",
  "value": [
    "<%", 
    "\tvar _Snippet = new ko.extensions.Snippet();", 
    "\t", 
    "", 
    "\t/* FIND VARIABLES*/", 
    "\t_Snippet.findVariablesIn();", 
    "\tvar variables\t= _Snippet.Variable.variables;", 
    "\t//alert( '_Snippet=\\n'+JSON.stringify( _Snippet, null, 2) );", 
    "\t//alert(_Snippet.line.content.before!=='');", 
    "\t/* MOVE TO NEW LINE */\t", 
    "\tif(_Snippet.line.content.before!=='')", 
    "\t\t_Snippet._moveKeywordToNewLine(); \t\t", 
    "\t", 
    "\t_Snippet.selectKeyword();", 
    "\tvar snippet_string = ''; \t", 
    "\t//alert( '_Snippet=\\n'+JSON.stringify( _Snippet, null, 2) )\t", 
    "\t//alert( 'variables=\\n'+JSON.stringify( variables, null, 2) )\t", 
    "\t", 
    "\t", 
    "\tif (variables.length > 0 ) {", 
    "\t\tsnippet_string = variables.map(function(variable, index){", 
    "\t\t\t", 
    "\t\t\t//alert( this.indexOf(variable) );", 
    "\t\t\tvar message = \"'[[%tabstop\"+(index+1)+\":\"+variable.name+\"]] : '+JSON.stringify( [[%tabstop\"+(index+1)+\":\"+variable.name+\"]], null, 4)\";", 
    "\t\t\treturn \"require('ko/console').info(\"+message+\");\";", 
    "\t\t}).join('\\n');", 
    "\t}else", 
    "\t\tsnippet_string\t= \"require('ko/console').info( [[%tabstop:[[%tabstop:key]]+]]'[[%tabstop1:object]]='+JSON.stringify( [[%tabstop1:object]][[%tabstop:]], null, 4));\";", 
    "\t\t//snippet_string\t= \"require('ko/console').info([[%tabstop:message]]);\";\t\t", 
    "\t", 
    "\t\t", 
    "\t\t//var snippet_string = 'X';", 
    "//alert( \"[[%tabstop1:object]]=\\nJSON.stringify( [[%tabstop1:object]], null, 4)", 
    "", 
    "%>", 
    "<%=snippet_string%>!@#_currentPos!@#_anchor"
  ], 
  "set_selection": "true", 
  "version": "1.0.12", 
  "treat_as_ejs": "true", 
  "icon": "file://C://GoogleDrive//Programs//code//Komodo//Icons//filetypes//Komodo//ejs.png", 
  "type": "snippet", 
  "indent_relative": "true", 
  "auto_abbreviation": "false"
