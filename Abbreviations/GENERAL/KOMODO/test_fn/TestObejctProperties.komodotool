  "name": "TestObejctProperties", 
  "value": [
    "<%", 
    "\tvar koFile\t= require(\"ko/file\");", 
    "\tvar class_name = koFile.basename(ko.extensions.getCurrentFilePath()).split(/\\./g).shift();", 
    "\tclass_name = '[[%tabstop1:'+class_name+']]';", 
    "", 
    "%>", 
    "/*\t-------------------------------------------------------------------------------------------------------------\t*/", 
    "/*\tTEST for <%=class_name%>\t*/", 
    "/*\tProperties test for object\t*/", 
    "/*\t-------------------------------------------------------------------------------------------------------------\t*/", 
    "", 
    "ko.extensions.<%=class_name%>Test = (function() {", 
    "\tvar\tTestObject\t= new ko.extensions.<%=class_name%>();", 
    "\tvar\ttest_properties\t= [[[%tabstop:'[[%tabstop:test_prop]]']]];", 
    "\tvar\ttest_methods\t= {", 
    "\t\t'[[%tabstop:[[%tabstop2:methodName]][[%tabstop:suffix]]]]': [[[%tabstop:'[[%tabstop:params]]']]],[[%tabstop:]]", 
    "\t};\t", 
    "\tif(ko.extensions.TestObejctProperties)", 
    "\t\t(new ko.extensions.TestObejctProperties(method_toggle=[[%tabstop:true]], properties_toggle=[[%tabstop:true]], clear=[[%tabstop:true]]))", 
    "\t\t\t.test(TestObject, test_methods, test_properties);", 
    "});", 
    "/* RUN TEST */", 
    "ko.extensions.<%=class_name%>Test();!@#_currentPos!@#_anchor"
  ], 
