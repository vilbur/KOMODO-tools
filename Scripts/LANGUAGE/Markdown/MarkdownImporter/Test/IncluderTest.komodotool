    "/** Loop all subdirs and find all files which name is same as folder name E.G.: \"FooBar\\FooBar.php\"", 
    " *", 
    " * Search extension is auto find from search_extensions variable", 
    " */", 
    "function includeFileStructureTest()", 
    "{", 
    "\tnew ko.extensions.vilbur.markdown.Includer()", 
    "\t\t\t\t\t\t\t\t\t\t.matchDirName()", 
    "\t\t\t\t\t\t\t\t\t\t.label('Structure')", 
    "\t\t\t\t\t\t\t\t\t\t.includeIndent();", 
    "};  ", 
    "", 
    "", 
    "/** Search files with name matching '-source' with extension 'md' in main directory", 
