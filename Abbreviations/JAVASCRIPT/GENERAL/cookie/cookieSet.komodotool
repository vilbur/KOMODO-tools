    "\tvar expires\t= \"expires=\" + ( value!=='' ?  myDate : \"Thu, 01 Jan 1970 00:00:01 GMT\" );", 
    "\t", 
    "\tdocument.cookie = key +\"=\" + value + \";\" + expires + '; path=/;';", 
    "}; !@#_currentPos!@#_anchor"
